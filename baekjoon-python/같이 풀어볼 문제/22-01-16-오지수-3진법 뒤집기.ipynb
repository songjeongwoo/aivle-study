   "source": [
    "#3진법 뒤집기\n",
    "\n",
    "\n",
    "def solution(a):\n",
    "    li=[]\n",
    "    sum=0\n",
    "    while a>0:\n",
    "        a,n = divmod(a,3)\n",
    "        li.append(str(n))\n",
    "    jin3= ''.join(li)\n",
    "    jin3= jin3[::-1]\n",
    "    for i in range(len(jin3)):\n",
    "        s1=(3**i)*int(jin3[i])\n",
    "        sum+=s1\n",
    "    return sum"
   ]
  }
