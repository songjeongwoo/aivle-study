   "source": [
    "# 음영 더하기\n",
    "def solution(abso, signs):\n",
    "    sum1 = 0\n",
    "    for i,j in zip(abso,signs):\n",
    "        if j == True:\n",
    "            sum1+=i\n",
    "        else :\n",
    "            sum1-=i\n",
    "    return(sum1)\n"
   ]
  }
