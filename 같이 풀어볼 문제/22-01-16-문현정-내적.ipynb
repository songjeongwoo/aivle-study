   "source": [
    "def solution(a, b):\n",
    "    ans = []\n",
    "    for i in range(0, len(a)):\n",
    "        c = a[i]*b[i]\n",
    "        ans.append(c)\n",
    "    answer = sum(ans)\n",
    "    return answer"
   ]
  }
