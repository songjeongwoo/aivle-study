    "def solution(n):\n",
    "    a = []\n",
    "    answer = 0\n",
    "    while n > 0:\n",
    "        a.append(n%3)\n",
    "        n = n // 3\n",
    "    #a.append(n)\n",
    "    \n",
    "    for i in range(0, len(a)):\n",
    "        answer += a[i]*3**(len(a)-i-1)\n",
    "    return answer"
   ]
